<Window x:Class="WpfSolarSystem.MainWindow" x:Name="spaceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfSolarSystem"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="Black" Name="Grid">
        <Canvas Name ="myCanvas" Margin="0, 0, 0, 0" Grid.ColumnSpan="2">
        </Canvas>
        <Canvas Name ="myRings" Margin="0,0,0,0"  Grid.ColumnSpan="2" Visibility="Visible">
        </Canvas>
        <Canvas Name ="myLabel" Margin="0, 0, 0, 0" Grid.ColumnSpan="2" Visibility="Visible">
        </Canvas>
        <Canvas Name ="myInfo" Margin="0, 0, 0, 0" Grid.ColumnSpan="2" Visibility="Visible">
        </Canvas>
    </Grid>
</Window>

